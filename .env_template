# ============================================================================
# AI Agent Orchestration Workshop - Environment Variables
# ============================================================================
# Copy this file to .env and fill in your values:
#   cp .env_template .env
# ============================================================================

# Your OpenAI API key (required for both frameworks)
OPENAI_API_KEY=sk-your-key-here

# ============================================================================
# Flock Configuration
# ============================================================================
# Model used by Flock agents (litellm format: provider/model)
DEFAULT_MODEL=openai/gpt-4.1

# ============================================================================
# Agent Framework Configuration
# ============================================================================
# Model used by Agent Framework's OpenAIResponsesClient
OPENAI_RESPONSES_MODEL_ID=gpt-4.1
